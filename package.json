{
  "name": "@icepeak/acc-shared-memory-reader",
  "version": "1.0.0",
  "description": "Node.js IPC Assetto Corsa Competizione shared memory reader",
  "keywords": [
    "assetto corsa",
    "acc",
    "ipc",
    "shared memory",
    "telemetry",
    "1cepeak"
  ],
  "homepage": "https://github.com/1cepeak/acc-shared-memory-reader",
  "bugs": "https://github.com/1cepeak/acc-shared-memory-reader/issues",
  "license": "MIT",
  "author": "Nikita Usov <foxwel.info@gmail.com>",
  "type": "module",
  "main": "dist/reader.cjs",
  "module": "dist/reader.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/reader.js",
      "require": "./dist/reader.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "^22.18.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lodash": "^4.17.21",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@fynnix/node-easy-ipc": "^1.1.4",
    "binutils": "^0.1.0",
    "utf8-bytes": "^0.0.1"
  }
}
